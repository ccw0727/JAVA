import java.util.*;

public class Main
{
	public static void main(String[] args) {
		Scanner keyin = new Scanner(System.in);
		
		System.out.print("＝＝歡迎使用數字轉換國字系統＝＝\n");
		System.out.print("請輸入一個整數數字 => ");
		int a ;
		String[] Chinese={"壹", "貳", "參", "肆", "伍", "陸", "柒", "捌", "玖", "拾"};
		a = keyin.nextInt();
		String C = Chinese[a-1];
	    
		System.out.printf("%s <=> %s ", a, C);
	
		
	}
}
